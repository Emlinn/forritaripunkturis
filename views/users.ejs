<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Forritari.is</title>
    <link rel="icon" type="image/ico" href="http://www.mysite.com/favicon.ico"/>


    <!-- Bootstrap -->
    <link href="/stylesheets/bootstrap.min.css" rel="stylesheet">
    <link href="http://getbootstrap.com/examples/carousel/carousel.css" rel="stylesheet">
    <link href="/stylesheets/hover.css" rel="stylesheet">
    <link href="/stylesheets/style.css" rel="stylesheet">


    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
  </head>
  <body>
    <!-- NAVBAR -->
      <div class="navbar navbar-default" navbar-fixed-top role="navigation">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="navbar-brand" href="/">Forritari.is</a>
        </div>
        <div class="navbar-collapse collapse">
          <ul class="nav navbar-nav navbar-right">
            <li><a href="/" class="hvr-underline-from-left">Heim</a></li>
            <li class="active"><a href="" class="hvr-underline-from-left">Um okkur</a></li>
            <li class="dropdown"><a href="#" class="dropdown-toggle" data-toggle="dropdown">Skrá inn<b class="caret"></b></a>
              <ul class="dropdown-menu">
                <li><a href="/login" class="hvr-underline-from-left">Innskráning</a></li>
                <li><a href="/signup" class="hvr-underline-from-left">Nýr notandi</a></li>
            </li>
        </div>
      </div>

    
    <div class="container marketing">
      <!-- Three columns of text below the carousel -->
      <div class="row">
        <div class="users col-lg-12">
          <img class="img-circle" src="http://www.fm-base.co.uk/forum/attachments/football-manager-2012-stories/216066d1326419781-forever-blowing-bubbles-unknown.gif" alt="Generic placeholder image" style="width: 140px; height: 140px;"><br><br>
          <%= usersprofile.local %>
          <% usersprofile.forEach( function( info ){ %>
          <p><%= info.local.fullname %></p>
          <p><%= info.local.birthday %></p>
          <p><a href="/<%= info._id %>/delete">Delete</a></p>
        <% }); %>
        </div><!-- /.col-lg-4 -->
        
      </div><!-- /.row -->
    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>
    <!-- Include all compiled plugins (below), or include individual files as needed -->
    <script src="/javascript/bootstrap.min.js"></script>
    <script src="/javascript/timer.js"></script>
  </body>
</html>